<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_weight="1"
        android:orientation="vertical"
        android:paddingTop="10dp"
        >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <LinearLayout
                android:id="@+id/ly_speed"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:layout_marginLeft="25dp"
                android:clickable="true"
                android:gravity="center"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:src="@drawable/dx_toolbox_embeded_icon_dashi_ad" />

                <TextView
                    android:id="@+id/tv_mainshow_obstacle"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:paddingTop="10dp"
                    android:text="我的钱包"
                    android:textColor="#2d3e66"
                    android:textSize="16dip" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/ly_filemanager"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:layout_marginLeft="25dp"
                android:clickable="true"
                android:gravity="center"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:src="@drawable/dx_toolbox_embeded_icon_dashi_ad" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:paddingTop="10dp"
                    android:text="考勤"
                    android:textColor="#2d3e66"
                    android:textSize="16dip" />

            </LinearLayout>

            <LinearLayout

                android:layout_width="80dp"
                android:layout_height="80dp"
                android:layout_marginLeft="25dp"
                android:clickable="true"
                android:gravity="center"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:src="@drawable/dx_toolbox_embeded_icon_dashi_ad" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:paddingTop="10dp"
                    android:text="公告通知"
                    android:textColor="#2d3e66"
                    android:textSize="16dip" />

            </LinearLayout>

            <LinearLayout

                android:layout_width="80dp"
                android:layout_height="80dp"
                android:layout_marginLeft="25dp"
                android:clickable="true"
                android:gravity="center"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:src="@drawable/dx_toolbox_embeded_icon_dashi_ad" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:paddingTop="10dp"
                    android:text="关于我们 "
                    android:textColor="#2d3e66"
                    android:textSize="16dip" />

            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:orientation="horizontal">

            <LinearLayout

                android:layout_width="80dp"
                android:layout_height="80dp"
                android:clickable="true"
                android:gravity="center"
                android:layout_marginLeft="25dp"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:src="@drawable/dx_toolbox_embeded_icon_dashi_ad" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:paddingTop="10dp"
                    android:text="联系我们"
                    android:textColor="#2d3e66"
                    android:textSize="16dip" />
            </LinearLayout>

            <LinearLayout

                android:layout_width="80dp"
                android:layout_height="80dp"
                android:clickable="true"
                android:gravity="center"
                android:layout_marginLeft="25dp"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:src="@drawable/dx_toolbox_embeded_icon_dashi_ad" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:paddingTop="10dp"
                    android:text="打卡"
                    android:textColor="#2d3e66"
                    android:textSize="16dip" />
            </LinearLayout>

            <LinearLayout

                android:layout_width="80dp"
                android:layout_height="80dp"
                android:clickable="true"
                android:gravity="center"
                android:layout_marginLeft="25dp"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:src="@drawable/dx_toolbox_embeded_icon_dashi_ad" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:paddingTop="10dp"
                    android:text="考勤"
                    android:textColor="#2d3e66"
                    android:textSize="16dip" />
            </LinearLayout>


            <LinearLayout

                android:layout_width="80dp"
                android:layout_height="80dp"
                android:clickable="true"
                android:gravity="center"
                android:layout_marginLeft="25dp"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:src="@drawable/dx_toolbox_embeded_icon_dashi_ad" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:paddingTop="10dp"
                    android:text="我的消息"
                    android:textColor="#2d3e66"
                    android:textSize="16dip" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:orientation="horizontal">

            <LinearLayout

                android:layout_width="80dp"
                android:layout_height="80dp"
                android:clickable="true"
                android:layout_marginLeft="25dp"
                android:gravity="center"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:src="@drawable/dx_toolbox_embeded_icon_dashi_ad" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:paddingTop="10dp"
                    android:text="电话"
                    android:textColor="#2d3e66"
                    android:textSize="16dip" />
            </LinearLayout>

            <LinearLayout

                android:layout_width="80dp"
                android:layout_height="80dp"
                android:clickable="true"
                android:layout_marginLeft="25dp"
                android:gravity="center"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:src="@drawable/toolbox_icon_dialer" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:paddingTop="10dp"
                    android:text="拨号"
                    android:textColor="#2d3e66"
                    android:textSize="16dip" />
            </LinearLayout>

            <LinearLayout

                android:layout_width="80dp"
                android:layout_height="80dp"
                android:clickable="true"
                android:gravity="center"
                android:layout_marginLeft="25dp"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:src="@drawable/dx_toolbox_embeded_icon_dashi_ad" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:paddingTop="10dp"
                    android:text="桌面"
                    android:textColor="#2d3e66"
                    android:textSize="16dip" />
            </LinearLayout>


            <LinearLayout

                android:layout_width="80dp"
                android:layout_height="80dp"
                android:clickable="true"
                android:layout_marginLeft="25dp"
                android:gravity="center"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:src="@drawable/dx_toolbox_embeded_icon_dashi_ad" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:paddingTop="10dp"
                    android:text="时间"
                    android:textColor="#2d3e66"
                    android:textSize="16dip" />
            </LinearLayout>
        </LinearLayout>

    </LinearLayout>

</LinearLayout>